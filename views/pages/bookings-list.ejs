<h2>Reservas</h2>
<a href="/bookings/new">+ Nova Reserva</a>
<table id="bookingsTable">
  <thead>
    <tr>
      <th>ID</th><th>Cliente</th><th>Sala</th><th>Data</th><th>In√≠cio</th><th>Fim</th>
    </tr>
  </thead>
  <tbody>
    <% if (bookings.length === 0) { %>
      <tr><td colspan="6">Nenhuma reserva.</td></tr>
    <% } else { %>
      <% bookings.forEach(b => { %>
        <tr>
          <td><%= b.id %></td>
          <td><%= b.client_id %></td>
          <td><%= b.room_id %></td>
          <td><%= b.date %></td>
          <td><%= b.start_time %></td>
          <td><%= b.end_time %></td>
        </tr>
      <% }) %>
    <% } %>
  </tbody>
</table>
